{
  "name": "my-stuff",
  "version": "1.0.0-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "prettier": "npx prettier . --write --ignore-unknown",
    "verify": "npm run prettier && ng lint && ng build && npm run test-headless"
  },
  "private": true,
